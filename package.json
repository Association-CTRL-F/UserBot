{
	"name": "userbot",
	"version": "25.7.7",
	"description": "UserBot",
	"main": "src/index.js",
	"scripts": {
		"dev": "tsx --tsconfig tsconfig.json src/index.ts",
		"dev:watch": "tsx watch --tsconfig tsconfig.json src/index.ts",
		"build": "tsc",
		"start": "node src/index.js",
		"lint": "eslint .",
		"format": "prettier --write .",
		"typecheck": "tsc --noEmit",
		"check": "pnpm run lint && pnpm run typecheck",
		"migrate": "kysely migrate:latest",
		"migrate:down": "kysely migrate:down",
		"migrate:create": "kysely migrate:make",
		"migrate:list": "kysely migrate:list",
		"seed:run": "kysely seed:run",
		"seed:make": "kysely seed:make",
		"sql": "kysely sql"
	},
	"imports": {
		"#app/*": "./src/app/*.js",
		"#lib/*": "./src/lib/*.js",
		"#database/*": "./src/database/*.js",
		"#repository/*": "./src/repository/*.js"
	},
	"dependencies": {
		"@sapphire/discord.js-utilities": "^7.3.3",
		"@sapphire/framework": "^5.3.6",
		"@sapphire/pieces": "^4.4.1",
		"@sapphire/plugin-hmr": "^3.0.2",
		"@sapphire/plugin-i18next": "^8.0.0",
		"@sapphire/plugin-logger": "^4.1.0",
		"@sapphire/plugin-subcommands": "^7.0.1",
		"@sapphire/utilities": "^3.18.2",
		"colorette": "^2.0.20",
		"discord.js": "~14.21.0",
		"dotenv": "^17.2.1",
		"envalid": "^8.1.0",
		"kysely": "^0.28.5",
		"pg": "^8.16.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.33.0",
		"@eslint/json": "^0.13.1",
		"@types/node": "^24.2.1",
		"@types/pg": "^8.15.5",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.4",
		"globals": "^16.3.0",
		"kysely-ctl": "^0.15.1",
		"prettier": "^3.6.2",
		"tsx": "^4.20.3",
		"typescript": "^5.9.2"
	},
	"type": "module",
	"volta": {
		"node": "24.1.0"
	}
}
