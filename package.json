{
	"name": "userbot",
	"version": "25.7.7",
	"description": "UserBot",
	"main": "src/index.js",
	"scripts": {
		"dev": "ts-node --project tsconfig.json -r tsconfig-paths/register src/index.ts",
		"build": "tsc",
		"start": "node dist/index.js",
		"lint": "eslint .",
		"format": "prettier --write .",
		"typecheck": "tsc --noEmit",
		"check": "pnpm run lint && pnpm run typecheck"
	},
	"imports": {
		"#app/*": "./src/app/*.ts"
	},
	"dependencies": {
		"@sapphire/discord.js-utilities": "^7.3.3",
		"@sapphire/framework": "^5.3.6",
		"@sapphire/plugin-logger": "^4.1.0",
		"discord.js": "~14.21.0",
		"envalid": "^8.1.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.31.0",
		"@eslint/json": "^0.13.1",
		"@types/node": "^24.1.0",
		"dotenv": "^17.2.1",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.3",
		"globals": "^16.3.0",
		"prettier": "^3.6.2",
		"ts-node": "^10.9.2",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0"
	},
	"type": "module",
	"volta": {
		"node": "24.1.0"
	}
}
