{
	"name": "userbot",
	"version": "3.15.0",
	"description": "Bot Capet",
	"main": "src/index.js",
	"scripts": {
		"start": "node .",
		"test": "eslint src && prettier --check src/**/*.js",
		"docker-start": "docker-compose -f ./docker/docker-compose.yml up -d",
		"docker-stop": "docker-compose -f ./docker/docker-compose.yml down -d",
