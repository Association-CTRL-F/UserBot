		"mysql2": "^2.3.3",
		"node-fetch": "^2.6.7"
	},
	"devDependencies": {
		"dotenv": "^16.0.0",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^2.6.2"
	},
	"type": "module"
}
